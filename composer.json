{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "^2.0",
        "braintree/braintree_php": "^6.11",
        "google/cloud-text-to-speech": "^1.5",
        "guzzlehttp/guzzle": "^7.8",
        "kingflamez/laravelrave": "*",
        "konekt/pdf-invoice": "^1.13",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.0",
        "midtrans/midtrans-php": "^2.5",
        "openai-php/laravel": "^0.10.0",
        "orhanerday/open-ai": "^5.0",
        "pragmarx/google2fa-laravel": "^2.0",
        "spatie/laravel-backup": "^9.0",
        "spatie/laravel-cookie-consent": "^3.3",
        "spatie/laravel-permission": "^6.0",
        "stripe/stripe-php": "^15.0",
        "yajra/laravel-datatables-oracle": "^11.0",
        "yoeunes/toastr": "^2.3",
        "yoomoney/yookassa-sdk-php": "^3.1",
        "zanysoft/laravel-zip": "^3.0",
        "zorb/promocodes": "^3.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Services/AdsenseService.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
